<% content_for :show do %>
<%= form_with model: readingroom do |f| %>
  

    <% readingroom.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
    <% end %>

  
  <div>

    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description%>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.submit %>
  </div>


<% end %>
<% end %>